#JSGF V1.0;

/**
 * JSGF Grammar
 */

grammar grammar;

public <letter> = (a | bee | sea | dee | ee | ef | gee | aitch | eye | jay | kay | ell | em
				   | en | oh | pee | cue | are | ess | tee | you | vee | ex | why | zee);
public <w> = (double you);
public <metachar> = (shift | control | alt | tab | escape | cap | windows | enter | backspace | delete | space);
public <command> = (/8/ press |/1/ hold |/1/ release) (<letter> | <w> | <metachar> | all);
